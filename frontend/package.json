{
  "name": "blogular",
  "description": "npm packages needed to build blogular",
  "private": true,
  "version": "0.0.0",
  "devDependencies": {
    "node-sass": "0.8.6",
    "browserify-shim": "3.4.1",
    "browserify": "3.46.0",
    "gulp-changed": "0.3.0",
    "gulp-livereload": "1.5.0",
    "gulp-notify": "1.2.5",
    "gulp-open": "0.2.8",
    "gulp": "3.6.2",
    "vinyl-source-stream": "0.1.1",
    "gulp-sass": "0.7.1",
    "gulp-angular-templatecache": "1.1.3",
    "express": "4.1.1",
    "morgan": "1.0.1",
    "bower": "1.3.3",
    "es6ify": "1.1.0",
    "gulp-size": "0.3.1",
    "http-proxy": "1.1.2",
    "gulp-clean": "0.2.4",
    "gulp-compile-handlebars": "0.2.0",
    "gulp-rename": "1.2.0",
    "envify": "1.2.1",
    "gulp-rev-all": "0.2.0",
    "gulp-uglify": "0.2.1",
    "gulp-if": "1.1.0",
    "gulp-streamify": "0.0.5",
    "gulp-concat-css": "0.1.4",
    "gulp-util": "2.2.14"
  },
  "browserify-shim": {
    "angular": {
      "exports": "angular",
      "depends": ["jquery"]
    },
    "jquery": {
      "exports": "jQuery"
    },
    "sockjs": {
      "exports": "SockJS"
    },
    "stomp": {
      "exports": "Stomp"
    },
    "epiceditor": {
      "exports": "EpicEditor"
    }
  },
  "scripts": {
    "install": "./node_modules/.bin/bower install"
  },
  "browser": {
    "angular": "./bower_components/angular/angular.js",
    "angular/route": "./bower_components/angular-route/angular-route.js",
    "angular/resource": "./bower_components/angular-resource/angular-resource.js",
    "angular/sanitize": "./bower_components/angular-sanitize/angular-sanitize.js",
    "angular/animate": "./bower_components/angular-animate/angular-animate.js",
    "angular/i18n/angular-locale_fi": "./bower_components/angular-i18n/angular-locale_fi.js",
    "angular-bootstrap/ui-bootstrap-tpls": "./bower_components/angular-bootstrap/ui-bootstrap-tpls.js",
    "bootstrap": "./bower_components/bootstrap/dist/js/bootstrap.js",
    "sockjs": "./bower_components/sockjs/sockjs.js",
    "stomp": "./bower_components/stomp-websocket/lib/stomp.js",
    "epiceditor": "./bower_components/epiceditor/src/editor.js",
    "marked": "./bower_components/marked/lib/marked.js",
    "jquery": "./bower_components/jquery/dist/jquery.js",
    "lodash": "./bower_components/lodash/dist/lodash.min.js",
    "templates": "./build/gulp/tmp/templates.js"
  }
}
